#include <stdio.h>

int* max_min(int arr[], int size){
    static int result[2]; 
    int max = arr[0], min = arr[0];
    for(int i = 1; i < size; i++){
        if(arr[i] > max){
            max = arr[i];
        }
        if(arr[i] < min){
            min = arr[i];
        }
    }
    result[0] = max; 
    result[1] = min; 
    return result; 
}

int main(){
    int arr[] = {4, 7, 1, 9, 2, 8, 3, 6, 10};
    int size = sizeof(arr)/sizeof(arr[0]);
    int* result = max_min(arr, size);
    printf("Maximum value: %d\nMinimum value: %d", result[0], result[1]);
    return 0;
} 

OUTPUT: Maximum value: 10
Minimum value: 1
