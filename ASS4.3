#include <stdio.h>

int main() {
   int lower, upper, sum = 0;
   printf("Enter the lower bound: ");
   scanf("%d", &lower);
   printf("Enter the upper bound: ");
   scanf("%d", &upper);
   for(int i = lower; i <= upper; i++) {
      if(i % 2 != 0) {
         sum += i;
      }
   }
   printf("The sum of all odd numbers from %d to %d is %d\n", lower, upper, sum);
   return 0;
}
