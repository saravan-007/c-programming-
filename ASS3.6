#include <stdio.h>

int isComposite(int num) {

    if (num <= 1) {

        return 0;

    }

    for (int i = 2; i <= num / 2; i++) {

        if (num % i == 0) {

            return 1;

        }

    }

    return 0;

}

int main() {

    int arr[] = {2, 4, 5, 6, 7, 8, 9, 10};

    int n = sizeof(arr) / sizeof(arr[0]);

    int compositeCount = 0;

    

    for (int i = 0; i < n; i++) {

        if (isComposite(arr[i])) {

            compositeCount++;

        }

    }

    printf("Number of composite numbers in the array: %d", compositeCount);

    return 0;

}

    
